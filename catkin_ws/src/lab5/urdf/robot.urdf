<?xml version="1.0"?>
<robot name="robot">

    <link name="world"></link>

    <joint name="world_link" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="world"/>
        <child link="base_link"/>
        <axis xyz="0.0 0.0 0.0"/> 
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
    </joint>

    <link name="base_link">
        <visual>
            <origin xyz="0.0 0.0 0.125" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.5" length="0.25"/>
            </geometry>
        </visual>
    </link>

    <joint name="baselink_link1" type="revolute">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="link1"/>
        <axis xyz="0.0 0.0 1"/> 
        <limit lower="-3.14" upper="3.14" effort="1000" velocity="0.5"/>
    </joint>

    <link name="link1">
        <visual>
            <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.3" length="0.5"/>
            </geometry>
        </visual>
    </link>

    <joint name="link1_link2" type="revolute">
        <origin xyz="0.0 0.0 0.75" rpy="0.0 0.0 0.0"/>
        <parent link="link1"/>
        <child link="link2"/>
        <axis xyz="0 1 0"/> 
        <limit lower="-3.14" upper="3.14" effort="1000" velocity="0.5"/>
    </joint>

    <link name="link2">
        <visual>
            <origin xyz="0.0 0.0 0.75" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="1.5"/>
            </geometry>
        </visual>
    </link>

    <joint name="link2_link3" type="revolute">
        <origin xyz="0.0 0.0 1.5" rpy="0.0 0.0 0.0"/>
        <parent link="link2"/>
        <child link="link3"/>
        <axis xyz="0 1 0"/> 
        <limit lower="-3.14" upper="3.14" effort="1000" velocity="0.5"/>
    </joint>

    <link name="link3">
        <visual>
            <origin xyz="0.0 0.0 0.75" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="1.5"/>
            </geometry>
        </visual>
    </link>
</robot>
